{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "action": {
    "default_title": "My Extension"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": ["content.js", "dialog.html", "dialog.css", "dialog.js","others.js","arrow.png","back.png","del.png","eraser.png","fill.png","line1.png","line2.png","line3.png","next.png","pen.png","picker.png","poly1.png","poly2.png","poly3.png","text.png"],
      "matches": ["<all_urls>"]
    }
  ]
}